<UserControl x:Class="AplikacjaDietetyczna.UserControls.UserControlPodsumowanie"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AplikacjaDietetyczna.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="AntiqueWhite">
            <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
              <TextBox FontFamily="Helvetica"
               x:Name="Użytkownik"
               FontSize="12"
               FontWeight="Light"
               HorizontalAlignment="Center"
               HorizontalContentAlignment="Left"
               Background="Transparent"
               Foreground="Black"
               BorderThickness="0"
               Width="147"
               Opacity="0.8"
               Text="Użytkownik:" Margin="29,70,1043.8,445"></TextBox>
              <TextBox FontFamily="Helvetica"
             x:Name="UżytkownikLogin"
             FontSize="12"
             FontWeight="Light"
             HorizontalAlignment="Center"
             HorizontalContentAlignment="Left"
             Background="Transparent"
             Foreground="Black"
             BorderThickness="0"
             Width="147"
             Opacity="0.8"
             Text="TestLogin" Margin="29,70,1043.8,445"></TextBox>
              </StackPanel>
              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
                <TextBox FontFamily="Helvetica"
                 x:Name="Plec"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="Płeć:" Margin="29,70,1043.8,445"></TextBox>
                <TextBox FontFamily="Helvetica"
                 x:Name="UżytkownikPlec"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="TestPlec" Margin="29,70,1043.8,445"></TextBox>
               </StackPanel>

              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
                <TextBox FontFamily="Helvetica"
                 x:Name="Wiek"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="Wiek:" Margin="29,70,1043.8,445"></TextBox>
                <TextBox FontFamily="Helvetica"
                 x:Name="UżytkownikWiek"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="TestWiek" Margin="29,70,1043.8,445"></TextBox>
              </StackPanel>

              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
                <TextBox FontFamily="Helvetica"
                 x:Name="Wzrost"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="Wzrost:" Margin="29,70,1043.8,445"></TextBox>
                <TextBox FontFamily="Helvetica"
                 x:Name="UżytkownikWzrost"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="TestWzrost" Margin="29,70,1043.8,445"></TextBox>
              </StackPanel>

              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
                <TextBox FontFamily="Helvetica"
                 x:Name="Waga"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="Waga:" Margin="29,70,1043.8,445"></TextBox>
                <TextBox FontFamily="Helvetica"
                 x:Name="UżytkownikWaga"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="TestWaga" Margin="29,70,1043.8,445"></TextBox>
              </StackPanel>
    </Grid>
</UserControl>
