               FontWeight="Light"
               HorizontalAlignment="Center"
               HorizontalContentAlignment="Left"
               Background="Transparent"
               Foreground="Black"
               BorderThickness="0"
               Width="147"
               Opacity="0.8"
               Text="Użytkownik:" Margin="29,70,1043.8,445"
               <!-- IsReadOnly="True"-->
               </TextBox>
              <TextBox FontFamily="Helvetica"
               x:Name="UżytkownikLogin"
               FontSize="12"
               FontWeight="Light"
               HorizontalAlignment="Center"
               HorizontalContentAlignment="Left"
               Background="Transparent"
               Foreground="Black"
               BorderThickness="0"
               Width="147"
               Opacity="0.8"
               Text="TestLogin" Margin="29,70,1043.8,445"
                <!-- IsReadOnly="True"-->
              </TextBox>
              </StackPanel>
              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
              <TextBox FontFamily="Helvetica"
               x:Name="Plec"
               FontSize="12"
               FontWeight="Light"
               HorizontalAlignment="Center"
               HorizontalContentAlignment="Left"
               Background="Transparent"
               Foreground="Black"
               BorderThickness="0"
               Width="147"
               Opacity="0.8"
               Text="Płeć:" Margin="29,70,1043.8,445"
              <!-- IsReadOnly="True"-->
              </TextBox>
               <TextBox FontFamily="Helvetica"
                x:Name="UżytkownikPlec"
                FontSize="12"
                FontWeight="Light"
                HorizontalAlignment="Center"
                HorizontalContentAlignment="Left"
                Background="Transparent"
                Foreground="Black"
                BorderThickness="0"
                Width="147"
                Opacity="0.8"
                Text="TestPlec" Margin="29,70,1043.8,445"
                 <!-- IsReadOnly="True"-->
               </TextBox>
               </StackPanel>

              <StackPanel Grid.ColumnSpan="2" Margin="0,0,-0.2,0">
                <TextBox FontFamily="Helvetica"
                 x:Name="Wiek"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="Płeć:" Margin="29,70,1043.8,445"
                  <!-- IsReadOnly="True"-->
                </TextBox>
                <TextBox FontFamily="Helvetica"
                 x:Name="UżytkownikWiek"
                 FontSize="12"
                 FontWeight="Light"
                 HorizontalAlignment="Center"
                 HorizontalContentAlignment="Left"
                 Background="Transparent"
                 Foreground="Black"
                 BorderThickness="0"
                 Width="147"
                 Opacity="0.8"
                 Text="TestWiek" Margin="29,70,1043.8,445"
                  <!-- IsReadOnly="True"-->
                </TextBox>
              </StackPanel>
    </Grid>
</UserControl>


