                    PreviewTextInput="NumberAndLettersValidationTextBox"
                    Watermark="Sztuka,100g,15ml"
                    MaxLength="20"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="268,109,700,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="KalorieText"
                    MaxLength="3"
                    Watermark="0-999kcal"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                    PreviewTextInput="NumberValidationTextBox"
                    Height="38" Margin="268,147,700,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="WeglowodanyText"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                   FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="268,261,700,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="TluszczeText"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="268,223,700,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="BialkaText"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="268,185,700,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <TextBox FontFamily="Helvetica"
                    FontSize="32"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Width="240"
                    Opacity="0.8"
                    Text="Nazwa Produktu:" Margin="604,71,376,531"
                    IsReadOnly="True"/>
                <xctk:WatermarkTextBox x:Name="SposobPodaniaText_Copy"
                    PreviewTextInput="NumberAndLettersValidationTextBox"
                    Watermark="Sztuka,100g,15ml"
                    MaxLength="20"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="854,109,114,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="KalorieText_Copy"
                    MaxLength="3"
                    Watermark="0-999kcal"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                    PreviewTextInput="NumberValidationTextBox"
                    Height="38" Margin="854,147,114,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="BialkaText_Copy"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="854,185,114,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="TluszczeText_Copy"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                   FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="854,223,114,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <xctk:WatermarkTextBox x:Name="WeglowodanyText_Copy"
                    PreviewTextInput="NumberValidationTextBox"
                    MaxLength="3"
                    Watermark="0-999g"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                    Height="38" Margin="854,261,114,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="252"/>
                <ComboBox HorizontalAlignment="Left"
                    x:Name="ProduktCombo"
                    ItemsSource="{Binding cbItems1}"
                    SelectedItem="{Binding SelectedcbItem1}"
                    Height="35"
                    Margin="854,74,0,0"
                    VerticalAlignment="Top"
                    Width="262" 
                    RenderTransformOrigin="-0.462,0.607" DropDownClosed="ProduktCombo_DownClosed"/>
                <Button x:Name="ZmienProdukt" Content="Zatwierdź zmiany" HorizontalAlignment="Left" Margin="604,330,0,0" VerticalAlignment="Top" Width="150" Height="50" RenderTransformOrigin="-0.22,0.466" Click="ZmienProdukt_Click"/>
                <Button x:Name="UsunProdukt" Content="Usuń" HorizontalAlignment="Left" Margin="854,330,0,0" VerticalAlignment="Top" Width="150" Height="50" RenderTransformOrigin="-0.22,0.466" Click="UsunProdukt_Click"/>


            </Grid>


        </StackPanel>







    </Grid>



</UserControl>