             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AplikacjaDietetyczna.UserControls" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="1220">

    <Grid x:Name="GridMain">
        <StackPanel>
            <Grid Background="#7aa9f6" Height="640" Margin="0,0,0.2,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>

                <StackPanel>
                    <Grid>


                        <TextBox FontFamily="Helvetica"
                         x:Name="TypPosilku"
                         FontSize="32"
                         FontWeight="DemiBold"
                         HorizontalAlignment="Center"
                         HorizontalContentAlignment="Left"
                         Background="Transparent"
                         Foreground="White"
                         BorderThickness="0"
                         Width="394"
                         Opacity="0.8"
                         Text="Dodaj własny produkt" Margin="29,11,797,483.6"
                         IsReadOnly="True"/>


                        <TextBox FontFamily="Helvetica"
                         x:Name="Sniadanie"
                         FontSize="32"
                         FontWeight="Light"
                         HorizontalAlignment="Left"
                         HorizontalContentAlignment="Left"
                         Background="Transparent"
                         Foreground="White"
                         BorderThickness="0"
                         Width="229"
                         Opacity="0.8"
                         Text="Sposób podania:" Margin="29,107,0,407"
                         IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Białka:" Margin="29,183,0,331"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy1"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Kalorie:" Margin="29,145,0,369"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy2"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Tłuszcze:" Margin="29,221,0,293"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy3"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Węglowodany:" Margin="29,259,0,255"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="TypPosilku_Copy"
                            FontSize="32"
                            FontWeight="DemiBold"
                            HorizontalAlignment="Center"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="466"
                            Opacity="0.8"
                            Text="Zmień lub usuń własny produkt" Margin="604,11,150,483"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy4"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Sposób podania:" Margin="604,107,0,407"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy5"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Kalorie:" Margin="604,145,0,369"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy6"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Białka:" Margin="604,183,0,331"
                            IsReadOnly="True"/>
                        <TextBox FontFamily="Helvetica"
                            x:Name="Sniadanie_Copy7"
                            FontSize="32"
                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Tłuszcze:" Margin="604,221,0,293"
                            IsReadOnly="True"/>
