                            FontWeight="Light"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Background="Transparent"
                            Foreground="White"
                            BorderThickness="0"
                            Width="229"
                            Opacity="0.8"
                            Text="Węglowodany:" Margin="604,259,0,255"
                            IsReadOnly="True"/>

                        <TextBox FontFamily="Helvetica"
                 
                         FontSize="18"
                         FontWeight="DemiBold"
                         HorizontalAlignment="Center"
                         HorizontalContentAlignment="Left"
                         Background="Transparent"
                         Foreground="White"
                         BorderThickness="0"
                         Width="606"
                         Opacity="0.8"
                         TextWrapping="Wrap"
                         Text="Jeśli Twojego produktu brakuje na liście to znaczy, że został od dodany do głównej bazy dla wszystkich użytkowników" Margin="604,407,10,85"
                         IsReadOnly="True"/>
                    </Grid>
                </StackPanel>





                <Button x:Name="DodajProdukt" Content="Dodaj Produkt" HorizontalAlignment="Left" Margin="27,330,0,0" VerticalAlignment="Top" Width="150" Height="50" RenderTransformOrigin="-0.22,0.466" Click="DodajProdukt_Click"/>
                <TextBox FontFamily="Helvetica"
                    FontSize="32"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Width="241"
                    Opacity="0.8"
                    Text="Nazwa Produktu:" Margin="27,71,952,531"
                    IsReadOnly="True"/>

                <xctk:WatermarkTextBox x:Name="NazwaPosilkuText"
                    PreviewTextInput="NumberAndLettersValidationTextBox"
                    MaxLength="55"
                    Watermark="Wprowadź nazwe"
                    FontFamily="Helvetica"
                    FontSize="28"
                    FontWeight="Light"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    Background="Transparent"
                    Foreground="White"
                    BorderThickness="0"
                    Opacity="0.8"
                         
                         Height="38" Margin="268,71,61,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="891"/>
                <Button x:Name="Cofnij" Content="Powrót" HorizontalAlignment="Left" Margin="27,416,0,0" VerticalAlignment="Top" Width="150" Height="50" RenderTransformOrigin="-0.22,0.466" Click="Cofnij_Click"/>
